Subject: don't build libnatpmp

---
 configure.ac            |    1 -
 third-party/Makefile.am |    1 -
 third-party/Makefile.in |    3 +--
 3 files changed, 1 insertions(+), 4 deletions(-)

diff --git a/configure.ac b/configure.ac
index 7e239bf..2c4a615 100644
--- a/configure.ac
+++ b/configure.ac
@@ -499,7 +499,6 @@ AC_CONFIG_FILES([Makefile
                  utils/Makefile
                  third-party/Makefile
                  third-party/miniupnp/Makefile
-                 third-party/libnatpmp/Makefile
                  third-party/libutp/Makefile
                  third-party/dht/Makefile
                  macosx/Makefile
diff --git a/third-party/Makefile.am b/third-party/Makefile.am
index 39eb5ee..3ca7b4b 100644
--- a/third-party/Makefile.am
+++ b/third-party/Makefile.am
@@ -4,7 +4,6 @@ endif
 
 SUBDIRS = \
   dht \
-  libnatpmp \
   miniupnp \
   $(UTP_DIR)
 
diff --git a/third-party/Makefile.in b/third-party/Makefile.in
index 672ff01..8e25424 100644
--- a/third-party/Makefile.in
+++ b/third-party/Makefile.in
@@ -69,7 +69,7 @@ AM_RECURSIVE_TARGETS = $(RECURSIVE_TARGETS:-recursive=) \
 	distdir
 ETAGS = etags
 CTAGS = ctags
-DIST_SUBDIRS = dht libnatpmp miniupnp libutp
+DIST_SUBDIRS = dht miniupnp libutp
 DISTFILES = $(DIST_COMMON) $(DIST_SOURCES) $(TEXINFOS) $(EXTRA_DIST)
 am__relativize = \
   dir0=`pwd`; \
@@ -286,7 +286,6 @@ transmissionlocaledir = @transmissionlocaledir@
 @BUILD_UTP_TRUE@UTP_DIR = libutp
 SUBDIRS = \
   dht \
-  libnatpmp \
   miniupnp \
   $(UTP_DIR)
 
